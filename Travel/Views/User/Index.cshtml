@model IEnumerable<Consult.Models.UserModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="home" id="home" style="margin-bottom:50px">
    <img src="~/lib/img/data.jpg" alt="" class="home__img" style="z-index:-999">
<br />
<br />
    <br /><br /><br />

    <h2 style="color:whitesmoke;-webkit-text-stroke: 1px var(--title-color);">Data Index</h2>
<br />

<table class="table">
        <tr style="color:black; font-weight:bolder; -webkit-text-stroke: 0.1px white; ">
     <!--
            <th>
                @Html.DisplayNameFor(model => model.Id)
                   </th>-->
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Place)
        </th>

        <!--
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Street)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zipcode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        -->

        <th>
            @Html.DisplayNameFor(model => model.From)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.To)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Member)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Room)
        </th>


    </tr>

    @foreach (var item in Model)
    {
            <tr style="color:black;">
                <!--
            <td>
                @Html.DisplayFor(modelItem => item.Id)
                </td>-->
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>


            <!--
            <td>
                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Street)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Zipcode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City)
            </td>
            -->


            <td>
                @Html.DisplayFor(modelItem => item.From)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.To)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Member)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Room)
            </td>
            
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id = item.Id  }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
</section>
